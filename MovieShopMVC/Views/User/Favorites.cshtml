@model List<ApplicationCore.Models.FavoriteModel>

@{
    <div class="rounded">
        <div class="container-fluid">
            <div class="row">
                @foreach (var movie in Model)
                {
                
                    <div class="col-xl-2 col-sm-4 col-lg-3 col-6">
                        <partial name="_MovieCard" model="movie.Movie"/>
                    </div>
                }
            </div>
        </div>
    </div>
    
}
